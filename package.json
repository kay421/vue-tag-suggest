{
  "name": "vue-tag-suggest",
  "version": "1.0.0",
  "description": "Vue.js tag search suggest components",
  "main": "dist/vue-tag-suggest.umd.js",
  "module": "dist/vue-tag-suggest.esm.js",
  "unpkg": "dist/vue-tag-suggest.min.js",
  "browser": {
    "./sfc": "src/vue-tag-suggest.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-tag-suggest.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-tag-suggest.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-tag-suggest.min.js"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
